<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.Subjects" table="subjects" schema="practice">
        <id name="idSubject">
            <column name="id_subject" sql-type="int"/>
        </id>
        <property name="nameSubject">
            <column name="name_subject" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>