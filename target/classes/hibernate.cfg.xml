<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-configuration PUBLIC
        "-//Hibernate/Hibernate Configuration DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
    <session-factory>
        <property name="connection.url">jdbc:mysql://localhost:3306/practice</property>
        <property name="connection.driver_class">com.mysql.cj.jdbc.Driver</property>
        <property name="connection.username">root</property>
        <property name="connection.password">root</property>
        <mapping class="com.example.Contacts"/>
        <mapping resource="Contacts.hbm.xml"/>
        <mapping class="com.example.ContactsDecoding"/>
        <mapping resource="ContactsDecoding.hbm.xml"/>
        <mapping resource="Documents.hbm.xml"/>
        <mapping class="com.example.Documents"/>
        <mapping class="com.example.LaborBook"/>
        <mapping resource="LaborBook.hbm.xml"/>
        <mapping class="com.example.Organization"/>
        <mapping resource="Organization.hbm.xml"/>
        <mapping class="com.example.Person"/>
        <mapping resource="Person.hbm.xml"/>
        <mapping resource="Personnel.hbm.xml"/>
        <mapping class="com.example.Personnel"/>
        <mapping class="com.example.Professions"/>
        <mapping resource="Professions.hbm.xml"/>
        <mapping resource="Subjects.hbm.xml"/>
        <mapping class="com.example.Subjects"/>
        <mapping class="com.example.TypeDoc"/>
        <mapping resource="TypeDoc.hbm.xml"/>
        <mapping class="com.example.Users"/>
        <mapping resource="Users.hbm.xml"/>
        <!-- <property name="connection.username"/> -->
        <!-- <property name="connection.password"/> -->

        <!-- DB schema will be updated if needed -->
        <!-- <property name="hibernate.hbm2ddl.auto">update</property> -->
    </session-factory>
</hibernate-configuration>