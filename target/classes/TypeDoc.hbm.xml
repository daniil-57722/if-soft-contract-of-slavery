<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.TypeDoc" table="type_doc" schema="practice">
        <id name="idDocumentType">
            <column name="id_document_type" sql-type="int"/>
        </id>
        <property name="documentName">
            <column name="document_name" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>