<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.Professions" table="professions" schema="practice">
        <id name="idProfession">
            <column name="id_profession" sql-type="int"/>
        </id>
        <property name="nameProfession">
            <column name="name_profession" sql-type="varchar(64)" length="64"/>
        </property>
        <property name="idSubject">
            <column name="id_subject" sql-type="int"/>
        </property>
    </class>
</hibernate-mapping>