<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="com.example.ContactsDecoding" table="contacts_decoding" schema="practice">
        <id name="idContact">
            <column name="id_contact" sql-type="int"/>
        </id>
        <property name="decoding">
            <column name="decoding" sql-type="varchar(45)" length="45"/>
        </property>
    </class>
</hibernate-mapping>